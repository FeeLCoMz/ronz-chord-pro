{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/practice(/.*)?",
      "dest": "/api/resources.js"
    },
    {
      "src": "/api/gigs(/.*)?",
      "dest": "/api/resources.js"
    },
    {
      "src": "/api/songs(/.*)?",
      "dest": "/api/songs.js"
    },
    {
      "src": "/api/setlists(/.*)?",
      "dest": "/api/setlists.js"
    },
    {
      "src": "/api/bands(/.*)?",
      "dest": "/api/bands.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
