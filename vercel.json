{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/auth/me",
      "dest": "/api/auth/me.js"
    },
    {
      "src": "/api/auth/(login|register|forgot-password|reset-password|2fa-setup|2fa-verify)",
      "dest": "/api/auth/index.js"
    },
    {
      "src": "/api/auth/change-password",
      "dest": "/api/auth/change-password.js"
    },
    {
      "src": "/api/bands/([^/]+)/members(?:/([^/]+))?",
      "dest": "/api/bands/members.js?id=$1&userId=$2"
    },
    {
      "src": "/api/bands/([^/]+)$",
      "dest": "/api/bands/[id].js?id=$1"
    },
    {
      "src": "/api/events/(gig|practice)/([^/]+)$",
      "dest": "/api/events/index.js?type=$1&id=$2"
    },
    {
      "src": "/api/events/(gig|practice)$",
      "dest": "/api/events/index.js?type=$1"
    },
    {
      "src": "/api/songs/([^/]+)$",
      "dest": "/api/songs/[id].js?id=$1"
    },
    {
      "src": "/api/setlists/([^/]+)$",
      "dest": "/api/setlists/index.js?id=$1"
    },    
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}